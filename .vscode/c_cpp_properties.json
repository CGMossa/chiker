{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/**",
                "${config:R.CMD.config.R_HOME}/include"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "STRICT_R_HEADERS",
                "R_NO_REMAP",
                "R_NO_REMAP_RMATH"
            ],
            "windowsSdkVersion": "10.0.22000.0",
            "compilerArgs": [
                "${config:R.CMD.config.cppflags}",
                "${config:R.CMD.config.ldflags}"
            ],
            "compilerPath": "${config:R.CMD.config.r_tools_soft}/bin/gcc",
            "cStandard": "c23",
            "cppStandard": "c++23",
            "intelliSenseMode": "windows-gcc-x64"
        }
    ],
    "version": 4
}